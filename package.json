{
	"name": "vscode-extension-webview-vue-sample",
	"description": "vscode-extension-webview-vue-sample",
	"version": "0.0.1",
	"publisher": "candywall",
	"private": true,
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onWebviewPanel:catCoding"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vue.open",
				"title": "open vue page"
			},
			{
				"command": "vue.doRefactor",
				"title": "doRefactor vue page"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm i && del-cli out -f && cd views && npm i && npm run build && cd .. && npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./",
		"build-page-watch": "cd views && npm run build-watch"
	},
	"devDependencies": {
		"@types/node": "^20.11.1",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"@typescript-eslint/eslint-plugin": "^5.42.0",
		"@typescript-eslint/parser": "^5.42.0",
		"eslint": "^8.26.0",
		"typescript": "^5.0.2",
		"vite": "^5.2.8",
		"del-cli": "^5.1.0"
	}
}
